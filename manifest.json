{
  "manifest_version": 2,
  "name": "Cookie Crumbler",
  "version": "0.1.0",
  "description": "__MSG_Automatically_delete_cookies_you_dont_want__",
  "default_locale": "en",
  "background": {
    "scripts": [
      "dist/js/background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "storage",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "browser_style": true,
    "default_title": "Cookie Crumbler",
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "64": "icons/icon-64.png",
      "128": "icons/icon-128.png",
      "256": "icons/icon-256.png"
    }
  },
  "options_ui": {
    "page": "html/settings.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  }
}
